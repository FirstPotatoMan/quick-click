{
    "manifest_version": 2,

    "name": "Quick Click",
    "description":
        "A clicking game chrome extension",
    "version": "1.0.0",

    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["js/eventPage.js"],
        "persistent": false
    },

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "128": "icon128.png"
    },

    "permissions": ["activeTab", "storage"]
}
